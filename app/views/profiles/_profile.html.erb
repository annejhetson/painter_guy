<%= form_for [@user, @profile], url: user_profile_path(@user, @profile) do |f| %>
	<div><%= f.label :Street %><br />
	<%= f.text_field :street %></div>

	<div><%= f.label :City %><br />
	<%= f.text_field :city %></div>

	<div><%= f.label :State %><br />
	<%= f.text_field :state %></div>

	<div><%= f.label :Zipcode %><br />
	<%= f.text_field :zip %></div>
	<%= f.hidden_field :user_id, value: current_user.id %><br>

	<%= f.submit "Update Profile", class: "btn btn-primary" %><br><br>

<% end %>